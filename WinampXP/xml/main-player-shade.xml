<groupdef id="player.shade.group.main" w="0" h="35" relatw="1">
	<layer id="background.shade.l" x="0" y="0" h="29" image="shade.background.main.left" activealpha="255" inactivealpha="0" sysregion="1"/>
	<layer id="background.shade.m" x="267" y="0" h="29" w="-538" relatw="1" image="shade.background.main.middle" activealpha="255" inactivealpha="0" sysregion="1"/>
	<layer id="background.shade.r" x="-271" y="0" h="29" relatx="1" image="shade.background.main.right" activealpha="255" inactivealpha="0" sysregion="1"/>
	
	<layer id="background.shade.l.i" x="0" y="0" h="29" image="shade.background.main.left.inactive" activealpha="0" inactivealpha="255" sysregion="1"/>
	<layer id="background.shade.m.i" x="267" y="0" h="29" w="-538" relatw="1" image="shade.background.main.middle.inactive" activealpha="0" inactivealpha="255" sysregion="1"/>
	<layer id="background.shade.r.i" x="-271" y="0" h="29" relatx="1" image="shade.background.main.right.inactive" activealpha="0" inactivealpha="255" sysregion="1"/>

	<layer id="pledit.region.top.left" x="0" y="0" image="window.shade.region.top.left" ghost="1" sysregion="-2"/>
	<layer id="pledit.region.top.right" x="-3" y="0" relatx="1" image="window.shade.region.top.right" ghost="1" sysregion="-2"/>
	<layer id="pledit.region.bottom.left" x="0" y="-3" relaty="1" image="window.shade.region.bottom.left" ghost="1" sysregion="-2"/>
	<layer id="pledit.region.bottom.right" x="-3" y="-3" relatx="1" relaty="1" image="window.shade.region.bottom.right" ghost="1" sysregion="-2"/>

	<text id="shade.window.titlebar.title.dropshadow" x="25" y="-3" w="600" h="42" text="WACUP" font="trebuc" fontsize="17"
    align="left" color="10,24,131" bold="1" antialias="1" activealpha="255" inactiveAlpha="0" forceuppercase="0"/>
	
  	<text id="shade.window.titlebar.title" x="24" y="-4" w="600" h="42" text="WACUP" font="trebuc" fontsize="17"
    align="left" color="255,255,255" bold="1" antialias="1" activealpha="255" inactiveAlpha="0" forceuppercase="0"/>
	
  	<text id="shade.window.titlebar.title.dim" x="24" y="-4" w="600" h="42" text="WACUP" font="trebuc" fontsize="17"
    align="left" color="216,228,248" bold="1" antialias="1" activealpha="0" inactivealpha="255" forceuppercase="0"/>

	<layer id="switcher.dummy" fitparent="1" image="none" dblclickaction="SWITCH;normal" move="1" cursor="cur.normal"/>

	<button id="player.button.mainmenu" action="SYSMENU" x="0" y="1" image="shade.button.mainmenu.n" hoverimage="shade.button.mainmenu.h" downimage="shade.button.mainmenu.d" tooltip="Winamp Menu" alpha="255" cursor="cur.normal"/>
	<slider id="player.slider.volume" action="VOLUME" x="137" y="10" w="71" h="14" orientation="horizontal" thumb="shade.button.slider" downthumb="shade.button.slider" ghost="1" alpha="0"/>

	<layer id="winamp.txt" x="27" y="2" image="shade.winamp.txt" activealpha="255" inactivealpha="100" ghost="1"/>
	<layer id="shade.bars" x="98" y="2" image="shade.bars" activealpha="255" inactivealpha="100" ghost="1"/>
	
	<group id="player.shade.group.display" x="118" y="0" ghost="1" cursor="cur.normal"/>
	
	<!--layer id="shade.buttons.bg" x="-268" y="2" relatx="1" image="shade.buttons.bg" cursor="cur.normal"/-->

	
	<!-- active buttons -->
	<button x="-192" y="1" relatx="1" image="shade.button.previous.n" activealpha="255" inactivealpha="0" cursor="cur.normal"/>
	<button id="pause.active" x="-168" y="1" relatx="1" image="shade.button.pause.n" activealpha="255" inactivealpha="0" cursor="cur.normal"/>
	<button id="play.active" x="-168" y="1" relatx="1" image="shade.button.play.n" activealpha="255" inactivealpha="0" cursor="cur.normal"/>
	<button x="-145" y="1" relatx="1" image="shade.button.stop.n" activealpha="255" inactivealpha="0" cursor="cur.normal"/>
	<button x="-122" y="1" relatx="1" image="shade.button.next.n" activealpha="255" inactivealpha="0" cursor="cur.normal"/>
	<button x="-100" y="1" relatx="1" image="shade.button.eject.n" activealpha="255" inactivealpha="0" cursor="cur.normal"/>
	
	<!-- inactive buttons -->
	<button x="-193" y="2" relatx="1" image="shade.button.previous.inactive" activealpha="0" inactivealpha="255" cursor="cur.normal"/>
	<button id="pause.inactive" x="-169" y="2" relatx="1" image="shade.button.pause.inactive" activealpha="0" inactivealpha="255" cursor="cur.normal"/>
	<button id="play.inactive" x="-169" y="2" relatx="1" image="shade.button.play.inactive" activealpha="0" inactivealpha="255" cursor="cur.normal"/>
	<button x="-146" y="2" relatx="1" image="shade.button.stop.inactive" activealpha="0" inactivealpha="255" cursor="cur.normal"/>
	<button x="-123" y="2" relatx="1" image="shade.button.next.inactive" activealpha="0" inactivealpha="255" cursor="cur.normal"/>
	<button x="-101" y="2" relatx="1" image="shade.button.eject.inactive" activealpha="0" inactivealpha="255" cursor="cur.normal"/>
	
	<button id="player.button.previous" action="PREV" x="-192" y="1" relatx="1" image="shade.button.transparent2731" hoverimage="shade.button.previous.h" downimage="shade.button.previous.d" tooltip="Pevious Track" cursor="cur.normal" alpha="255"/>
	<button id="player.button.pause" action="PAUSE" x="-168" y="1" relatx="1" image="shade.button.transparent2631" hoverimage="shade.button.pause.h" downimage="shade.button.pause.d" tooltip="Pause" cursor="cur.normal" alpha="255"/>
	<button id="player.button.play" action="PLAY" x="-168" y="1" relatx="1" image="shade.button.transparent2631" hoverimage="shade.button.play.h" downimage="shade.button.play.d" tooltip="Play" cursor="cur.normal" alpha="255"/>
	<button id="player.button.stop" action="STOP" x="-145" y="1" relatx="1" image="shade.button.transparent2631" hoverimage="shade.button.stop.h" downimage="shade.button.stop.d" tooltip="Stop" cursor="cur.normal" alpha="255"/>
	<button id="player.button.next" action="NEXT" x="-122" y="1" relatx="1" image="shade.button.transparent2731" hoverimage="shade.button.next.h" downimage="shade.button.next.d" tooltip="Next Track" cursor="cur.normal" alpha="255"/>
  	<button id="player.button.eject" action="EJECT" x="-100" y="1" relatx="1" image="shade.button.transparent3031" hoverimage="shade.button.eject.h" downimage="shade.button.eject.d" tooltip="Open file(s)" cursor="cur.normal" alpha="255"/>

	<grid id="seek.bg" x="-245" y="0" h="30" w="50" relatx="1" left="shade.seek.bg.left" middle="shade.seek.bg.middle" right="shade.seek.bg.right" move="0" alpha="255" tooltip="Seeking Bar" cursor="cur.normal"/>

	<slider id="player.slider.seek.shadow" action="SEEK" x="-247" y="2" w="55" h="29" relatx="1" orientation="horizontal" thumb="shade.seek.slider.shadow" downthumb="shade.seek.slider.shadow" alpha="250"/>	
	<slider id="player.slider.seek" action="SEEK" x="-247" y="2" w="55" h="29" relatx="1" orientation="horizontal" thumb="shade.seek.slider.n" downthumb="shade.seek.slider.d" tooltip="Seeking Bar" alpha="255" cursor="cur.posbar"/>
	<slider id="player.slider.seek.ghost" action="SEEK" x="-247" y="2" w="55" h="29" relatx="1" orientation="horizontal" thumb="shade.seek.slider.n" downthumb="shade.seek.slider.d" tooltip="Seeking Bar" alpha="255" cursor="cur.posbar"/>

	<layer id="player.resizer.mid" x="-4" y="0" w="4" h="35" image="shade.resizer" relatx="1" move="0" resize="right" alpha="255" tooltip="Adjust Horizontal Size" rectrgn="1" cursor="cur.posbar"/>
	
	<!-- active -->
	<button x="-73" y="1" relatx="1" image="shade.button.minimize.n" cursor="cur.close" activealpha="255" inactivealpha="0"/>
	<button x="-50" y="1" relatx="1" image="shade.button.switch.n" cursor="cur.close" activealpha="255" inactivealpha="0"/>
	<button x="-27" y="1" relatx="1" image="shade.button.close.n" cursor="cur.close" activealpha="255" inactivealpha="0"/>

	<!-- inactive -->
	<button x="-73" y="1" relatx="1" image="shade.button.minimize.inactive" cursor="cur.close" activealpha="0" inactivealpha="255"/>
	<button x="-50" y="1" relatx="1" image="shade.button.switch.inactive" cursor="cur.close" activealpha="0" inactivealpha="255"/>
	<button x="-27" y="1" relatx="1" image="shade.button.close.inactive" cursor="cur.close" activealpha="0" inactivealpha="255"/>

	<button id="player.button.minimize" action="MINIMIZE" x="-73" y="1" relatx="1" image="shade.button.transparent" hoverimage="shade.button.minimize.h" downimage="shade.button.minimize.d" tooltip="Minimize" cursor="cur.normal" alpha="255"/>
	<button id="player.button.switch" action="SWITCH" param="NORMAL" x="-50" y="1" relatx="1" image="shade.button.transparent" hoverimage="shade.button.switch.h" downimage="shade.button.switch.d" tooltip="Toggle Windowshade Mode" cursor="cur.normal" alpha="255"/>
	<button id="player.button.close" action="CLOSE" x="-27" y="1" relatx="1" image="shade.button.transparent" hoverimage="shade.button.close.h" downimage="shade.button.close.d" tooltip="Close" cursor="cur.close" alpha="255"/>

  <script file="..\WinampXP\scripts\play_pause.maki" param="player.button.play;player.button.pause"/>	
</groupdef>

<groupdef id="player.shade.group.display" w="-384" h="33" relatw="1">
  <grid id="player.display" x="0" y="3" h="29" w="0" relatw="1" left="shade.background.display.left" middle="shade.background.display.middle" right="shade.background.display.right" ghost="1" />

	<button id="vismenu.menuentry1" x="7" y="9" h="0" w="0" action="TOGGLE" param="guid:avs" image="none" cursor="cur.normal"/>
	<button id="vismenu.menuentry2" x="7" y="9" h="0" w="0" action="Vis_Cfg" image="none"/>
	<button id="vismenu.menuentry3" x="7" y="9" h="0" w="0" action="WA5:Prefs" param="33" image="none"/>

	<!--rect id="plssayer.vis" x="8" y="10" w="72" h="15" color="0,0,250" filled="0"/-->
	<vis id="shade.vis" x="8" y="10" w="72" h="16" alpha="255" cursor="cur.normal" gammagroup="visualizers"/>
  <!-- rect id="hide.for.gordon" x="79" y="9" w="2" h="19" color="20,18,28" filled="1" ghost="1" gammagroup="display"/ -->
	<!--layer id="player.vis.blocker" x="8" y="10" w="72" h="15" image="none" cursor="cur.normal"/-->
	
	<text id="shade.time" display="TIME" x="80" y="5" w="86" h="20" font="trebuc" fontsize="21" timecolonwidth="9" timeroffstyle="0" align="center" 
	 color="white" forcefixed="1" bold="1" dblclickaction="trackinfo" cursor="cur.normal" tooltip="Time Display (click to toggle elapsed/remaining)"/>
	
	<Songticker id="shade.display.songname" x="175" y="5" w="-179" h="20" relatw="1" showlen="1" font="trebuc" fontsize="18" align="center" 
	rightclickaction="trackmenu" color="white" dblclickaction="trackinfo" alpha="255" cursor="cur.normal" tooltip="Song Title (right-click for options)"/>
	
	<text id="player.display.actioninfo" x="175" y="5" w="-179" h="20" relatw="1" font="trebuc" fontsize="18" align="center" color="white" alpha="255" cursor="cur.normal"/>

  <!--script file="scripts\visualizer.maki" param="shade.vis;player.vis.trigger;hide.for.vic"/-->
  <script file="..\WinampXP\scripts\pauseblink.maki"/>
</groupdef>

<layout id="shade" w="550" h="29" minimum_w="550" minimum_h="29" maximum_w="10000" desktopalpha="1" droptarget="pldr" linkwidth="normal" appbar="top|bottom">
	<group id="player.shade.group.main" x="0" y="0" cursor="cur.normal"/>
</layout>